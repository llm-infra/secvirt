{
  "hasSeenIdeIntegrationNudge": true,
  "mcpServers": {
    "lsp-mcp": {
      "command": "lsp-mcp",
      "args": [
        "serve"
      ],
      "transport": "stdio",
      "description": "Language Server Protocol MCP 集成",
      "trust": true,
      "timeout": 3600000
    }
  },
  "selectedAuthType": "custom-llm-api",
  "useRipgrep": true,
  "historyStorage": {
    "type": "elasticsearch",
    "embeddingService": {
      "url": "https://api.siliconflow.cn/v1/embeddings",
      "headers": {
        "Authorization": "Bearer sk-nezsfwoxbewsokaxnegfnlwnaxqxzxrvjwmubveqmrkuvpyv",
        "Content-Type": "application/json"
      },
      "requestTemplate": {
        "model": "BAAI/bge-m3",
        "input": "${texts}",
        "encoding_format": "float"
      },
      "responsePath": "data",
      "embeddingField": "embedding"
    },
    "reranker": {
      "enabled": true,
      "url": "https://api.siliconflow.cn/v1/rerank",
      "headers": {
        "Authorization": "Bearer sk-nezsfwoxbewsokaxnegfnlwnaxqxzxrvjwmubveqmrkuvpyv",
        "Content-Type": "application/json"
      },
      "model": "BAAI/bge-reranker-v2-m3",
      "topN": 5,
      "returnDocuments": true
    },
    "maxEntries": 100000,
    "cleanupDays": 365
  }
}